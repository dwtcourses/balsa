agraph","content":[{"type":"text","text":"File1"}]},{"type":"todo_list","content":[{"type":"todo_item","attrs":{"done":false},"content":[{"type":"paragraph","content":[{"type":"text","text":"File2"}]}]},{"type":"todo_item","attrs":{"done":false},"content":[{"type":"paragraph","content":[{"type":"text","text":"File3"}]}]}]},{"type":"paragraph"},{"type":"paragraph"},{"type":"paragraph","content":[{"type":"text","text":"as"}]},{"type":"paragraph","content":[{"type":"text","text":"a"}]},{"type":"paragraph","content":[{"type":"text","text":"sd"}]},{"type":"paragraph","content":[{"type":"image","attrs":{"src":"http://describe.im:3000/uploads/userUploads/as_EnaMj--EE5DZLQUEAAL62m.jpg","alt":null,"title":null}}]},{"type":"paragraph","content":[{"type":"text","text":"asdasd asd"}]},{"type":"paragraph"},{"type":"paragraph"},{"type":"paragraph"},{"type":"paragraph"},{"type":"paragraph"},{"type":"paragraph","content":[{"type":"text","text":"Table 12"}]},{"type":"paragraph"},{"type":"paragraph"},{"type":"paragraph"},{"type":"paragraph","content":[{"type":"text","text":"asdasd"}]},{"type":"paragraph"},{"type":"paragraph"},{"type":"paragraph"}]}},
  {"name": "üìóWeekly Meeting Notes Template", "categories": [1, 2, 3], "content": {"type":"doc","content":[{"type":"title","content":[{"type":"text","text":"üìóWeekly Meeting Notes Template "}]},{"type":"blockquote","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"},{"type":"italic"}],"text":"Customize for your needs :-)"}]}]},{"type":"paragraph"},{"type":"table","content":[{"type":"table_row","content":[{"type":"table_cell","attrs":{"colspan":3,"rowspan":1,"colwidth":null,"background":null},"content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"üóìCreated:"},{"type":"text","text":" December 20, 2019 10:15"}]},{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"üë®‚ÄçüíªBy:"},{"type":"text","text":" Mike Nicolson"}]},{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"üôá‚Äç‚ôÄÔ∏èParticipants: "},{"type":"text","text":"@ "},{"type":"mention","attrs":{"id":5,"label":"Gordon Thomson"}},{"type":"text","text":" "}]}]}]}]},{"type":"paragraph"},{"type":"paragraph","content":[{"type":"text","text":"A weekly meeting template can be used to capture all information gathered during your "},{"type":"text","marks":[{"type":"comment","attrs":{"guid":"d6458660-0003-4933-8ec1-0ee0eb4c1660"}}],"text":"company"},{"type":"text","text":" meetings. Check who was there, when the meeting was done and what the outcomes were. "}]},{"type":"paragraph"},{"type":"heading","attrs":{"level":3},"content":[{"type":"text","text":"üí°Discussion items:"}]},{"type":"bullet_list","content":[{"type":"list_item","content":[{"type":"paragraph","content":[{"type":"text","text":"Make sure we get prepared for the next client meeting in Berlin"}]}]},{"type":"list_item","content":[{"type":"paragraph","content":[{"type":"text","text":"Prepare a product presentation customized for the client"}]}]}]},{"type":"heading","attrs":{"level":3},"content":[{"type":"text","text":"‚öíAction items:"}]},{"type":"todo_list","content":[{"type":"todo_item","attrs":{"done":false},"content":[{"type":"paragraph","content":[{"type":"text","text":"Modify presentation template to use correct colors"}]}]},{"type":"todo_item","attrs":{"done":true},"content":[{"type":"paragraph","content":[{"type":"text","text":"Get help from designer to come up with the new design"}]}]},{"type":"todo_item","attrs":{"done":true},"content":[{"type":"paragraph","content":[{"type":"text","text":"Double check contents"}]}]},{"type":"todo_item","attrs":{"done":false},"content":[{"type":"paragraph","content":[{"type":"text","text":"Share with the client one day before the presentation"}]}]}]},{"type":"paragraph"}]}},
